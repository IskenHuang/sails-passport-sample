<div class="container clearfix content">
    <% if(isAuth) { %>
        <h1>Hello <%- me.account %></h1>
        <a href="/api/auth/logout"><%- i18n('logout') %></a>
    <% }else{ %>
        <a href="/signin"><%- i18n('signin') %></a>
        <a href="/signup"><%- i18n('signup') %></a>
    <% } %>
</div>
